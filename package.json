  "homepage": "https://maripab.github.io/photo-comparison/admin",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.7.23",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.12",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^8.0.1",
    "mongodb-memory-server": "^6.6.1",
    "multer": "^1.4.2",
    "onchange": "^7.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.48",
    "typescript": "^3.9.5",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.7",
    "npm-run-all": "^4.1.5",
    "unique-string": "^2.0.0"
  }
